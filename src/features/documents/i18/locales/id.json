{
  "title": "Document",
  "upload": {
    "button": {
      "placeholder": "Upload Document"
    },

    "dialog": {
      "name": "upload-documents",
      "title": "Upload",
      "description": "You are about to upload document. Please upload your document(s):",
      "tab": {
        "name": "upload_tab-documents",
        "data": [
          {
            "id": "drag_and_drop",
            "name": "Drag and Drop or Browse"
          },
          {
            "id": "url",
            "name": "Upload via url"
          }
        ]
      },
      "browse": {
        "description": "You are about to upload document. Please upload your document(s):",
        "input": {
          "name": "drag_n_drop_file_input-document",
          "message": "Drag and drop or {{actions}} files",
          "description": "Support: pdf, jpg, jpeg and max size 10 mb",
          "actions": "browse"
        },
        "preview": {
          "actions": "Remove File"
        },
        "setting": {
          "label": "Document Setting",
          "input": {
            "private_document": {
              "label": "Private Document",
              "name": "private_document-url-documents"
            }
          }
        },
        "actions": {
          "primary": {
            "text": "Upload"
          },
          "secondary": {
            "text": "Cancel"
          }
        }
      },
      "url": {
        "description": "You are about to upload document. Please upload your document(s):",
        "input": {
          "name": "url_file_input-document",
          "placeholder": "e.g https://drive.google.com"
        },
        "setting": {
          "label": "Document Setting",
          "input": {
            "private_document": {
              "label": "Private Document",
              "name": "private_document-documents"
            }
          }
        },
        "actions": {
          "primary": {
            "text": "Upload"
          },
          "secondary": {
            "text": "Cancel"
          }
        }
      }
    }
  },
  "data_table": {
    "name": "documents",
    "initial": {
      "message": "Get started to uploading document"
    },
    "empty": {
      "message": "Data is empty"
    },
    "search": {
      "input": {
        "name": "search_documents",
        "placeholder": "Search File Name"
      }
    },
    "head": {
      "data": [
        {
          "id": "no",
          "label": "No"
        },
        {
          "id": "file_name",
          "label": "File Name"
        },
        {
          "id": "date",
          "label": "Date"
        }
      ]
    }
  }
}
